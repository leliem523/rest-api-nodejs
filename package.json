{
	"name": "nodejs-mysql",
	"version": "0.0.0",
	"private": false,
	"scripts": {
		"start": "nodemon --delay 500ms --exec babel-node src/app.js",
		"clean": "rm -rf dist",
		"build": "yarn clean && babel ./src --out-dir dist --copy-files"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged --allow-empty"
		}
	},
	"lint-staged": {
		"src/**/*.+(jsx|js|ts|tsx|md|json|yml|yaml)": [
			"prettier --write --ignore-unknown",
			"eslint --fix",
			"git add -A"
		]
	},
	"dependencies": {
		"bcrypt": "^5.0.1",
		"body-parser": "^1.19.1",
		"cookie-parser": "~1.4.4",
		"debug": "~2.6.9",
		"express": "~4.16.1",
		"http-errors": "~1.6.3",
		"jade": "~1.11.0",
		"jsonwebtoken": "^8.5.1",
		"morgan": "~1.9.1",
		"mysql2": "^2.3.3",
		"sequelize": "^6.12.0-beta.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.17.6",
		"@babel/core": "^7.17.5",
		"@babel/eslint-parser": "^7.17.0",
		"@babel/node": "^7.16.8",
		"@babel/preset-env": "^7.16.11",
		"babel-plugin-import": "^1.13.3",
		"babel-plugin-module-resolver": "^4.1.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^4.3.8",
		"lint-staged": "^12.3.5",
		"nodemon": "^2.0.15",
		"prettier": "^2.5.1"
	}
}
